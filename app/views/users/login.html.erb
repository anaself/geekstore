<h1>Login In</h1>

<div>
    <%= form_for @user, url: {action: "login_verify"} do |f| %>

      <div>
        <%= f.label :email %>
        <%= f.email_field :email %>
      </div>

      <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
      </div>

      <%= f.submit "Create my account"%>
    <% end %>

</div>